<resources>
    <string name="app_name_full">通话录音配置</string>

    <!-- Preference headers -->
    <string name="pref_header_general">常规</string>
    <string name="pref_header_about">关于</string>
    <string name="pref_header_debug">调试</string>

    <!-- General preferences -->
    <string name="pref_call_recording_name">通话录音</string>
    <string name="pref_call_recording_desc">对来电和去电进行通话录音，录音时需要麦克风和通知权限</string>

    <string name="pref_record_rules_name">自动录音规则</string>
    <string name="pref_record_rules_desc">设置哪些通话将被自动录音</string>

    <string name="pref_output_dir_name">保存目录</string>
    <string name="pref_output_dir_desc">选择一个目录用来保存录音文件</string>

    <string name="pref_output_format_name">保存格式</string>
    <string name="pref_output_format_desc">选择录音存储编码格式</string>

    <string name="pref_min_duration_name">最短录音时长</string>
    <plurals name="pref_min_duration_desc">
        <item quantity="one">仅保留至少 %d 秒的录音</item>
        <item quantity="other">仅保留至少 %d 秒的录音</item>
    </plurals>
    <string name="pref_min_duration_desc_zero">保留任意长度的录音</string>

    <string name="pref_write_metadata_name">保存元数据文件</string>
    <string name="pref_write_metadata_desc">创建同名 JSON 文件包含通话录音详情</string>

    <string name="pref_record_telecom_apps_name">录制集成电信框架的应用</string>
    <string name="pref_record_telecom_apps_desc">录制与安卓电信系统集成的第三方应用通话。这些通话的录音很可能只包含静音</string>

    <string name="pref_record_dialing_state_name">在接通前开始录音</string>
    <string name="pref_record_dialing_state_desc">对于拨出电话，在拨号开始时立即录音，而不是等待通话接通</string>

    <string name="pref_show_launcher_icon_name">显示桌面图标</string>
    <string name="pref_show_launcher_icon_desc">隐藏桌面图标后，拨打 <b>*#*#227#*#*</b> 打开此设置页面</string>

    <!-- About "preference" -->
    <string name="pref_version_name">软件版本</string>

    <!-- Debug preferences -->
    <string name="pref_force_direct_boot_name">强制直接启动模式</string>
    <string name="pref_force_direct_boot_desc">模拟设备处于首次解锁前的状态以进行测试。</string>

    <string name="pref_migrate_direct_boot_name">迁移直接启动录音</string>
    <string name="pref_migrate_direct_boot_desc">迁移首次解锁前生成的录音。通常在首次解锁后自动完成。</string>

    <string name="pref_save_logs_name">保存日志</string>
    <string name="pref_save_logs_desc">将 logcat 日志保存到文件中。</string>

    <!-- Pick contact group activity -->
    <string name="pick_contact_group_title">选择联系人分组</string>
    <string name="pick_contact_group_local_group">本地分组</string>

    <!-- Title of the preference category holding the "Add new rule" option and the list of record rules. -->
    <string name="pref_header_rules">规则</string>
    <!-- Message shown above the record rules list that indicates what rules do and how to interact with the list UI. -->
    <string name="record_rules_info">规则决定了匹配通话后采取的动作。系统将自上而下处理直到找到匹配项。长按可重新排序，滑动可删除规则</string>
    <!-- Title of preference option for adding a new record rule. -->
    <string name="pref_add_new_rule_name">添加新规则</string>
    <!-- Description of preference option for adding a new record rule. -->
    <string name="pref_add_new_rule_desc">添加规则以确定如何处理匹配的通话</string>
    <!-- Numeric title for every custom rule (excluding the default rule). The placeholder value (Arabic numerals) is a number starting at 1. -->
    <string name="record_rules_list_custom_name">规则 %d</string>
    <!-- Title for the default/fallback rule at the bottom of the list. -->
    <string name="record_rules_list_default_name">默认规则</string>
    <!-- Accessibility option to move a rule up by one spot in the list. -->
    <string name="record_rules_list_action_move_up">上移规则</string>
    <!-- Accessibility option to move a rule down by one spot in the list. -->
    <string name="record_rules_list_action_move_down">下移规则</string>
    <!-- Accessibility option to remove a rule. -->
    <string name="record_rules_list_action_remove">移除规则</string>

    <!-- Button in the record rule editor to select a different contact. -->
    <string name="record_rule_editor_change_contact">更改联系人</string>
    <!-- Button in the record rule editor to select a different contact group. -->
    <string name="record_rule_editor_change_contact_group">更改联系人分组</string>

    <!-- Title of the record rule editor section for configuring the call number to match. -->
    <string name="record_rule_number_title">电话号码</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_number_any_chip">任意</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_number_unknown_chip">未知</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. This represents contact rules as a category, not a specific contact. -->
    <string name="record_rule_number_contact_chip">联系人</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. This represents contact group rules as a category, not a specific contact group. -->
    <string name="record_rule_number_contact_group_chip">联系人分组</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Match: " prefix for visual consistency. -->
    <string name="record_rule_number_unknown_summary">匹配：未知号码</string>
    <!-- Summary shown in the record rules list descriptions. This intentionally has no "Match: " prefix because it already has another prefix. When the contacts permission is denied, this will display an internal contact ID instead of the contact name. -->
    <string name="record_rule_number_contact_summary">联系人：%s</string>
    <!-- Summary shown in the record rules list descriptions. This intentionally has no "Match: " prefix because it already has another prefix. This variant is only used when the contacts permission is denied and only the group ID is available (not the group name or account). -->
    <string name="record_rule_number_contact_group_unknown_summary">联系人分组：%s</string>
    <!-- Summary shown in the record rules list descriptions. This intentionally has no "Match: " prefix because it already has another prefix. -->
    <string name="record_rule_number_contact_group_with_account_summary">联系人分组：%1$s (%2$s)</string>

    <!-- Title of the record rule editor section for configuring the type of call to match. -->
    <string name="record_rule_type_title">通话类型</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_type_any_chip">任意</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_type_incoming_chip">来电</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_type_outgoing_chip">去电</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_type_conference_chip">电话会议</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Type: " prefix for visual consistency. -->
    <string name="record_rule_type_incoming_summary">类型：呼入通话</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Type: " prefix for visual consistency. -->
    <string name="record_rule_type_outgoing_summary">类型：拨出通话</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Type: " prefix for visual consistency. -->
    <string name="record_rule_type_conference_summary">类型：电话会议</string>

    <!-- Title of the record rule editor section for configuring which SIM slot to match. -->
    <string name="record_rule_sim_title">SIM 卡</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_sim_any_chip">任意</string>
    <!-- Text for the UI chip to select a specific SIM slot in the rule editor. The placeholder value (Arabic numerals) is a number starting at 1. -->
    <string name="record_rule_sim_specific_chip">SIM %d</string>
    <!-- Summary shown in the record rules list descriptions. This should have a prefix, like other summary lines, for visual consistency. The placeholder value (Arabic numerals) is a number starting at 1. -->
    <string name="record_rule_sim_specific_summary">SIM 卡: %d</string>

    <!-- Title of the record rule editor section to configure the action to take when all conditions match. -->
    <string name="record_rule_action_title">动作</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_action_save_chip">保存</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_action_discard_chip">舍弃</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_action_ignore_chip">忽略</string>
    <!-- Description shown for this action in the rule editor. The text can be as verbose as necessary. -->
    <string name="record_rule_action_save_desc">保存录音。通话期间显示的通知可将录音标记为通话结束后删除</string>
    <!-- Description shown for this action in the rule editor. The text can be as verbose as necessary. -->
    <string name="record_rule_action_discard_desc">通话结束后舍弃录音。通话期间显示的通知可手动保留录音</string>
    <!-- Description shown for this action in the rule editor. The text can be as verbose as necessary. -->
    <string name="record_rule_action_ignore_desc">完全忽略该通话。不会进行录音，且无法在通话中手动开始录制</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Action: " prefix for visual consistency. -->
    <string name="record_rule_action_save_summary">动作：保存录音</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Action: " prefix for visual consistency. -->
    <string name="record_rule_action_discard_summary">动作：舍弃录音</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Action: " prefix for visual consistency. -->
    <string name="record_rule_action_ignore_summary">动作：忽略通话</string>

