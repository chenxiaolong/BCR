<resources>
    <string name="app_name_full">Basic Call Recorder</string>

    <!-- Preference headers -->
    <string name="pref_header_general">Général</string>
    <string name="pref_header_about">À propos</string>
    <string name="pref_header_debug">Débogage</string>

    <!-- General preferences -->
    <string name="pref_call_recording_name">Enregistrement d\'appel</string>
    <string name="pref_call_recording_desc">Enregistre les appels entrants et sortants. Les permissions microphone et notification sont nécessaires pour l\'enregistrement en arrière plan.</string>

    <string name="pref_record_rules_name">Règles d\'auto-enregistrement</string>
    <string name="pref_record_rules_desc">Configurer quels appels doivent être automatiquement enregistrés.</string>

    <string name="pref_output_dir_name">Dossier des enregistrements</string>
    <string name="pref_output_dir_desc">Choisir un dossier pour stocker les enregistrements. Appui long pour ouvrir dans le gestionnaire de fichiers.</string>

    <string name="pref_output_format_name">Format des enregistrements</string>
    <string name="pref_output_format_desc">Choisir un encodage pour stocker les enregistrements.</string>

    <string name="pref_min_duration_name">Durée minimum d\'enregistrement</string>
    <plurals name="pref_min_duration_desc">
        <item quantity="one">Garder les enregistrements durant au moins %d seconde.</item>
        <item quantity="other">Garder les enregistrements durant au moins %d secondes.</item>
    </plurals>
    <string name="pref_min_duration_desc_zero">Garder les enregistrements de toutes durées.</string>

    <string name="pref_write_metadata_name">Écriture fichier metadata</string>
    <string name="pref_write_metadata_desc">Créer un fichier JSON contenant les détails des appels avec le fichier audio.</string>

    <string name="pref_record_telecom_apps_name">Enregistrer les applications tierces</string>
    <string name="pref_record_telecom_apps_desc">Enregistrer les appels provenant des applications tierces. Ces enregistrements d\'appel devraient contenir uniquement du silence.</string>

    <string name="pref_record_dialing_state_name">Enregistrer l\'appel avant décrochage</string>
    <string name="pref_record_dialing_state_desc">Pour les appels sortant, démarrer l\'enregistrement dès le début d\'appel, avant que le correspondant décroche.</string>

    <string name="pref_show_launcher_icon_name">Afficher l\'icone de l\'application</string>
    <string name="pref_show_launcher_icon_desc">Lorsque l\'icone du lanceur est masquée, composer <b>*#*#BCR#*#*</b> pour ouvrir l\'application.</string>

    <!-- About "preference" -->
    <string name="pref_version_name">Version</string>

    <!-- Debug preferences -->
    <string name="pref_force_direct_boot_name">Forcer le mode démarrage direct</string>
    <string name="pref_force_direct_boot_desc">Simuler l\'appareil dans l\'état précédant le premier déverrouillage pour les tests.</string>

    <string name="pref_migrate_direct_boot_name">Migrer les enregistrements du démarrage direct</string>
    <string name="pref_migrate_direct_boot_desc">Migrer les enregistrements réalisés avant le premier déverrouillage. Cela devrait se produire normalement après le premier déverrouillage.</string>

    <string name="pref_save_logs_name">Enregistrer les logs</string>
    <string name="pref_save_logs_desc">Enregistrer les journeaux logs dans un fichier.</string>

    <!-- Pick contact group activity -->
    <string name="pick_contact_group_title">Choisir un groupe de contact</string>
    <string name="pick_contact_group_local_group">Groupe local</string>

    <!-- Title of the preference category holding the "Add new rule" option and the list of record rules. -->
    <string name="pref_header_rules">Règles</string>
    <!-- Message shown above the record rules list that indicates what rules do and how to interact with the list UI. -->
    <string name="record_rules_info">Les règles déterminent les actions à effectuer pour les appels correspondants. Elles sont appliquées de haut en bas jusqu\'à une correspondance trouvée. Maintenir pour les réorganiser ou balayer pour les supprimer.</string>
    <!-- Title of preference option for adding a new record rule. -->
    <string name="pref_add_new_rule_name">Ajouter une nouvelle règle</string>
    <!-- Description of preference option for adding a new record rule. -->
    <string name="pref_add_new_rule_desc">Ajouter une règle définissant l\'action à effectuer pour l\'appel correspondant.</string>
    <!-- Numeric title for every custom rule (excluding the default rule). The placeholder value (Arabic numerals) is a number starting at 1. -->
    <string name="record_rules_list_custom_name">Règle %d</string>
    <!-- Title for the default/fallback rule at the bottom of the list. -->
    <string name="record_rules_list_default_name">Règle par défaut</string>
    <!-- Accessibility option to move a rule up by one spot in the list. -->
    <string name="record_rules_list_action_move_up">Remonter la règle</string>
    <!-- Accessibility option to move a rule down by one spot in the list. -->
    <string name="record_rules_list_action_move_down">Descendre la règle</string>
    <!-- Accessibility option to remove a rule. -->
    <string name="record_rules_list_action_remove">Supprimer la règle</string>

    <!-- Button in the record rule editor to select a different contact. -->
    <string name="record_rule_editor_change_contact">Changer contact</string>
    <!-- Button in the record rule editor to select a different contact group. -->
    <string name="record_rule_editor_change_contact_group">Changer groupe de contact</string>

    <!-- Title of the record rule editor section for configuring the call number to match. -->
    <string name="record_rule_number_title">Numéro d\'appel</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_number_any_chip">Tous</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_number_unknown_chip">Inconnu</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. This represents contact rules as a category, not a specific contact. -->
    <string name="record_rule_number_contact_chip">Contact</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. This represents contact group rules as a category, not a specific contact group. -->
    <string name="record_rule_number_contact_group_chip">Groupe de contact</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Match: " prefix for visual consistency. -->
    <string name="record_rule_number_unknown_summary">Correspondance : Numéros inconnus</string>
    <!-- Summary shown in the record rules list descriptions. This intentionally has no "Match: " prefix because it already has another prefix. When the contacts permission is denied, this will display an internal contact ID instead of the contact name. -->
    <string name="record_rule_number_contact_summary">Contact : %s</string>
    <!-- Summary shown in the record rules list descriptions. This intentionally has no "Match: " prefix because it already has another prefix. This variant is only used when the contacts permission is denied and only the group ID is available (not the group name or account). -->
    <string name="record_rule_number_contact_group_unknown_summary">Groupe de contact : %s</string>
    <!-- Summary shown in the record rules list descriptions. This intentionally has no "Match: " prefix because it already has another prefix. -->
    <string name="record_rule_number_contact_group_with_account_summary">Groupe de contact : %1$s (%2$s)</string>

    <!-- Title of the record rule editor section for configuring the type of call to match. -->
    <string name="record_rule_type_title">Type d\'appel</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_type_any_chip">Tous</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_type_incoming_chip">Entrant</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_type_outgoing_chip">Sortant</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_type_conference_chip">Groupé</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Type: " prefix for visual consistency. -->
    <string name="record_rule_type_incoming_summary">Type : Appel entrant</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Type: " prefix for visual consistency. -->
    <string name="record_rule_type_outgoing_summary">Type : Appel sortant</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Type: " prefix for visual consistency. -->
    <string name="record_rule_type_conference_summary">Type : Appel groupé</string>

    <!-- Title of the record rule editor section for configuring which SIM slot to match. -->
    <string name="record_rule_sim_title">Emplacement SIM</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_sim_any_chip">Tous</string>
    <!-- Text for the UI chip to select a specific SIM slot in the rule editor. The placeholder value (Arabic numerals) is a number starting at 1. -->
    <string name="record_rule_sim_specific_chip">SIM %d</string>
    <!-- Summary shown in the record rules list descriptions. This should have a prefix, like other summary lines, for visual consistency. The placeholder value (Arabic numerals) is a number starting at 1. -->
    <string name="record_rule_sim_specific_summary">Emplacement SIM : %d</string>

    <!-- Title of the record rule editor section to configure the action to take when all conditions match. -->
    <string name="record_rule_action_title">Action</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_action_save_chip">Sauvegarder</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_action_discard_chip">Rejeter</string>
    <!-- Text for the UI chip to select this element in the rule editor. The text should be as short as possible. -->
    <string name="record_rule_action_ignore_chip">Ignorer</string>
    <!-- Description shown for this action in the rule editor. The text can be as verbose as necessary. -->
    <string name="record_rule_action_save_desc">Sauvegarder l\'enregistrement. L\'enregistrement peut être sélectionné pour suppression à la fin de l\'appel via la notification durant l\'échange.</string>
    <!-- Description shown for this action in the rule editor. The text can be as verbose as necessary. -->
    <string name="record_rule_action_discard_desc">Rejeter l\'enregistrement à la fin de l\'appel. L\'enregistrement peut être conservé via la notification durant l\'échange.</string>
    <!-- Description shown for this action in the rule editor. The text can be as verbose as necessary. -->
    <string name="record_rule_action_ignore_desc">Ignorer complètement l\'appel. L\'appel ne sera pas enregistré et il sera impossible de démarrer l\'enregistrement pendant l\'échange.</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Action: " prefix for visual consistency. -->
    <string name="record_rule_action_save_summary">Action : Sauvegarder l\'enregistrement</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Action: " prefix for visual consistency. -->
    <string name="record_rule_action_discard_summary">Action : Rejeter l\'enregistrement</string>
    <!-- Summary shown in the record rules list descriptions. This should include a "Action: " prefix for visual consistency. -->
    <string name="record_rule_action_ignore_summary">Action : Ignorer l\'appel</string>

