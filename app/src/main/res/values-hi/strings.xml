    <!-- Filename template dialog -->
    <string name="filename_template_dialog_title">फ़ाइल नाम टेम्पलेट</string>
    <!-- NOTE: For the "supported_vars" annotation, the content MUST not be empty. Any string can go inside of it as a placeholder. -->
    <string name="filename_template_dialog_message">आउटपुट फ़ाइल नाम के लिए एक कस्टम टेम्पलेट दर्ज करें। चर घुंघराले ब्रेसिज़ के साथ निर्दिष्ट किए गए हैं (eg. <annotation type="template">{var}</annotation>)। फ़ॉलबैक को वर्गाकार कोष्ठक के साथ निर्दिष्ट किया गया है (eg. <annotation type="template">[{contact_name}|Unknown]</annotation>)।\n\nSupported variables: <annotation type="supported_vars">PLACEHOLDER</annotation>। सिंटैक्स के संपूर्ण विवरण के लिए <annotation type='template_docs'>दस्तावेज़ीकरण</annotation> देखें।</string>
    <string name="filename_template_dialog_warning_subdirectories">एंड्रॉइड स्टोरेज एक्सेस फ्रेमवर्क के खराब प्रदर्शन के कारण, उपनिर्देशिकाओं का उपयोग किया जा रहा है (<annotation type="template">/</annotation> character) कुछ डिवाइस पर रिकॉर्डिंग सहेजते समय महत्वपूर्ण विलंब हो सकता है। देरी कॉल के अंत में होती है और इसके परिणामस्वरूप कॉल ऑडियो का कोई नुकसान नहीं होना चाहिए।</string>
    <string name="filename_template_dialog_error_empty">टेम्प्लेट खाली नहीं हो सकता</string>
    <string name="filename_template_dialog_error_unknown_variable">अज्ञात टेम्पलेट वैरिएबल: <annotation type="template">PLACEHOLDER</annotation></string>
    <string name="filename_template_dialog_error_has_argument">वेरिएबल में कोई तर्क नहीं हो सकता: <annotation type="template">PLACEHOLDER</annotation></string>
    <string name="filename_template_dialog_error_invalid_argument">अमान्य चर तर्क: <annotation type="template">PLACEHOLDER</annotation></string>
    <string name="filename_template_dialog_error_invalid_syntax">टेम्प्लेट सिंटैक्स अमान्य है</string>
    <string name="filename_template_dialog_action_reset_to_default">डिफ़ॉल्ट पर पुनः सेट करें</string>

