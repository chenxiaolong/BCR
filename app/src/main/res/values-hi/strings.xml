<resources>
    <string name="app_name_full">Basic Call Recorder</string>

    <!-- Preference headers -->
    <string name="pref_header_general">सामान्य</string>
    <string name="pref_header_about">के बारे में</string>

    <!-- General preferences -->
    <string name="pref_call_recording_name">कॉल रिकॉर्डिंग</string>
    <string name="pref_call_recording_desc">इनकमिंग और आउटगोइंग फ़ोन कॉल रिकॉर्ड करें. पृष्ठभूमि में रिकॉर्डिंग के लिए माइक्रोफ़ोन और अधिसूचना अनुमतियाँ आवश्यक हैं।</string>

    <string name="pref_record_rules_name">ऑटो-रिकॉर्ड नियम</string>
    <string name="pref_record_rules_desc">कॉन्फ़िगर करें कि कौन सी कॉल स्वचालित रूप से रिकॉर्ड की जानी चाहिए।</string>

    <string name="pref_output_dir_name">उत्पादन निर्देशिका</string>
    <string name="pref_output_dir_desc">रिकॉर्डिंग संग्रहीत करने के लिए एक निर्देशिका चुनें। फ़ाइल मैनेजर में खोलने के लिए देर तक दबाएँ।</string>

    <string name="pref_output_format_name">आउटपुट स्वरूप</string>
    <string name="pref_output_format_desc">रिकॉर्डिंग के लिए एक एन्कोडिंग प्रारूप चुनें।</string>

    <string name="pref_inhibit_batt_opt_name">बैटरी अनुकूलन अक्षम करें</string>
    <string name="pref_inhibit_batt_opt_desc">सिस्टम द्वारा ऐप के बंद होने की संभावना कम हो जाती है।</string>

    <string name="pref_write_metadata_name">मेटाडेटा फ़ाइल लिखें</string>
    <string name="pref_write_metadata_desc">ऑडियो फ़ाइल के बगल में कॉल के बारे में विवरण वाली एक JSON फ़ाइल बनाएं।</string>

    <!-- About "preference" -->
    <string name="pref_version_name">वर्जन:</string>

    <!-- Record rules bottom sheet -->
    <string name="pref_header_rules">नियम</string>
    <string name="pref_add_rule_name">नियम जोड़ें</string>
    <string name="pref_add_rule_desc">किसी संपर्क के लिए ऑटो-रिकॉर्ड नियम जोड़ें।</string>
    <string name="record_rules_rule_added">नया नियम जोड़ा गया। नियम को हटाने के लिए उसे देर तक दबाएँ।</string>
    <string name="record_rules_rule_exists">नियम पहले से मौजूद है।</string>
    <string name="record_rules_info">जब कोई नियम चालू होता है, तो कॉल डिफ़ॉल्ट रूप से स्वचालित रूप से रिकॉर्ड हो जाती है और अधिसूचना से मैन्युअल रूप से हटाया जा सकता है। जब कोई नियम बंद कर दिया जाता है, तो कॉल के अंत में रिकॉर्डिंग हटा दी जाएगी जब तक कि इसे अधिसूचना में पुनर्स्थापना बटन का उपयोग करके संरक्षित नहीं किया जाता है।</string>
    <string name="record_rule_type_all_calls_name">तमाम कॉल्स</string>
    <string name="record_rule_type_all_calls_desc">अधिक विस्तृत नियमों के लिए संपर्क अनुमति आवश्यक है।</string>
    <string name="record_rule_type_all_other_calls_name">अन्य सभी कॉल</string>
    <string name="record_rule_type_all_other_calls_desc">ऐसी कॉलें जो किसी अन्य नियम से मेल नहीं खातीं।</string>
    <string name="record_rule_type_unknown_calls_name">अज्ञात कॉल</string>
    <string name="record_rule_type_unknown_calls_desc">ऐसी कॉलें जो किसी भी संपर्क से मेल नहीं खातीं।</string>
    <string name="record_rule_type_contact_name">संपर्क: %s</string>
    <string name="record_rule_type_contact_desc">नियम हटाने के लिए देर तक दबाएँ।</string>

